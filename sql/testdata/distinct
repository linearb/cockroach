statement ok
CREATE TABLE xyz (
  x INT PRIMARY KEY,
  y INT,
  z INT,
  INDEX foo (z, y)
)

statement ok
INSERT INTO xyz VALUES (1, 2, 3), (2, 5, 6), (3, 2, 3), (4, 5, 6), (5, 2, 6), (6, 3, 5), (7, 2, 9);

query II
SELECT y,z FROM xyz
----
2 3
5 6
2 3
5 6
2 6
3 5
2 9

query II
SELECT DISTINCT y,z FROM xyz
----
2 3
5 6
2 6
3 5
2 9

query II
SELECT DISTINCT ON (y) y, z FROM xyz
----
2 3
5 6
3 5

query II
SELECT DISTINCT ON (z) y, z FROM xyz
----
2 3
5 6
3 5
2 9

query II
SELECT DISTINCT ON (y + z) y, z FROM xyz
----
2 3
5 6
2 6

query II
SELECT DISTINCT ON (y + z) y, z FROM xyz ORDER BY (y + z)
----
2 3
2 6
2 9
